{
    "manifest_version": 3,
    "name": "CatAstrophe",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "64": "icon64.png",
        "128": "icon128.png"
      }
    }, 
    "content scripts": [
      {
      "matches": ["https://www.youtube.com/*", "https://www.google.com/*"],
      "'js": ["./main.js"]
      }
    ]
  }
  
 

